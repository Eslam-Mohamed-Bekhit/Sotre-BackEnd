{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./build --onSuccess 'node ./build/server.js'",
    "start": "tsc && node build/server.js",
    "migration": "db-migrate",
    "jasmine": "jasmine-ts  --config=./spec/support/jasmine.json",
    "test": "export NODE_ENV=test  && db-migrate up --config ./database.json -e test && tsc && npm run jasmine && db-migrate down",
    "test:windows": "set NODE_ENV=test&& db-migrate up --config ./database.json -e test && tsc && npm run jasmine && db-migrate down",
    "lint": "eslint . --ext .ts",
    "format": "npm run lint --fix",
    "start:dev": "nodemon --watch ./**/*.ts --exec ts-node ./src/server.ts"
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.9",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^11.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/jasmine": "^3.10.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.8",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.3.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.1",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9"
  }
}
